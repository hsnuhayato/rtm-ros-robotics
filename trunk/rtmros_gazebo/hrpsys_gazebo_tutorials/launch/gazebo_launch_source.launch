<launch>
  <arg name="gzname" default="gazebo"/>
  <arg name="WORLD" default="__world_name__"/>
  <arg name="HRPSYS_GAZEBO_CONFIG" default="$(find hrpsys_gazebo_tutorials)/config/__robot_name__.yaml" />

  <include file="$(find hrpsys_gazebo_tutorials)/launch/gazebo_robot_no_controllers.launch">
    <arg name="gzname" value="$(arg gzname)" />
    <arg name="WORLD" value="$(arg WORLD)" />
    <arg name="HRPSYS_GAZEBO_CONFIG" value="$(arg HRPSYS_GAZEBO_CONFIG)" />
  </include>
</launch>
