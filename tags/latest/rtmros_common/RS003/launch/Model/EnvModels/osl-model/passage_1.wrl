#VRML V2.0 utf8
################################################################################
# つくば第２事業所(通路) -１-
################################################################################
Transform {
	translation 0 0 0
	rotation 0 0 1 0
	scale 1 1 1
	children [
		#-----------------------------------------------------------------------
		# 床
		#-----------------------------------------------------------------------
		Shape {
			appearance Appearance {
				material Material {
					specularColor 1 1 1
					diffuseColor 1.000000 1.000000 1.000000
					ambientIntensity 0
					transparency 0.000000
					shininess 0.200000
					emissiveColor 0 0 0
				}
			}
			geometry IndexedFaceSet {
				coord Coordinate {
					point [
						0   0 0,    # 0
						6.4 0 0,    # 1
						6.4 0 3.5,  # 2
						0   0 3.5   # 3
					]
				}
				coordIndex [
					0 1 2 3 -1
				]
				solid FALSE
			}
		}
		#-----------------------------------------------------------------------
		# 壁2
		#-----------------------------------------------------------------------
		Transform {
			# ダミー
			translation 0.5 1.495 2.55 #dmyhalf
			rotation 0 0 1 0
			scale 1 1 1
			children [
				Shape {
					geometry Box {
						# ダミー(柱の実測値)
						size 1 2.99 0.1 #dmyfull
					}
					appearance Appearance {
						material Material {
							diffuseColor      1 1 1
						}
					}
				}
			]
		}
		#-----------------------------------------------------------------------
		# 壁3
		#-----------------------------------------------------------------------
