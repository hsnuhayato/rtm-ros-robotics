<launch>
  <arg name="KILL_SERVERS" default="false" />
  <include file="$(find hrpsys)/launch/hrpsys.launch">
    <arg name="PROJECT_FILE" value="$(find hrpsys_ros_bridge_tutorials)/models/SampleRobot.xml" />
    <arg name="MODEL_FILE" value="$(find openhrp3)/build/OpenHRP-3/sample/model/sample1.wrl" />
    <arg name="CONF_FILE" value="$(find hrpsys_ros_bridge_tutorials)/models/SampleRobot.conf" />
    <arg name="SIMULATOR_NAME" value="SampleRobot(Robot)0" />
    <arg name="KILL_SERVERS" default="$(arg KILL_SERVERS)" />
  </include>
</launch>
