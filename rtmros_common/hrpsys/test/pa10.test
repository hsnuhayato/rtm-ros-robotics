<launch>
  <include file="$(find hrpsys)/launch/pa10.launch" />
  <node name="pa10_python_client" pkg="hrpsys" type="hrpsyspy"
	args="$(find hrpsys)/share/hrpsys/samples/PA10/PA10.py" respawn="true" />
  <test test-name="pa10" pkg="openhrp3" time-limit="200"
   type="sample-projects.py" args="--capture $(find hrpsys)/test/pa10-grxui.png --max-time 30 --kill-nameserver"/>
</launch>
